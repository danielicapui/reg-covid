def rgb2gray(rgb):
  return np.dot(rgb[..., :3], [0.2989, 0.5870, 0.1140])

def open_image(response, size):
  image=Image.open(response)
  image = image.resize((size, size), Image.ANTIALIAS)
  return rgb2gray(np.array(image.convert('RGB')))  # .tolist()

